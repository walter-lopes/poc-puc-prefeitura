@model SGM_MVC.Models.Cidadao.Person;

@{
    ViewData["Title"] = "Informações Imóvel";
}

<div>
    <h4>Informações do Proprietário / Imóvel</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            CPF / CNPJ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Identifier)
        </dd>
        <dt class="col-sm-2">
            Endereço
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Address.Street)
        </dd>
        <dt class="col-sm-2">
            Número
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Address.Number)
        </dd>
        <dt class="col-sm-2">
            CEP
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Address.PostalCode)
        </dd>
        <dt class="col-sm-2">
            Cidade
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Address.City)
        </dd>
        <dt class="col-sm-2">
            Estado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Address.State)
        </dd>
        <dt class="col-sm-2">
            Tipo de Imóvel
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.Kind)
        </dd>
        <dt class="col-sm-2">
            Tipo de Imposto
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.KindOfFee)
        </dd>
        <dt class="col-sm-2">
            Valor do Imposto
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Property.FeeValue)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="IptuItr" class="btn btn-primary">Voltar</a>
</div>
